      sysproc.c:
	92) // add a path to the pathvariable in exec.c
	    int
	    sys_add_path(void)
	    {
	      char *path;
	      uint i;
		if(argstr(0, &path) < 0)
		  return -1;
		for(i=0 ; i<MAX_PATH_ENTRIES ; i++)
		{
		  if(pathVariable[i]==0)
		  {
		    pathVariable[i]= path;
		    return 0;
		  }
		}
		return -1;
	    }
